\mode<presentation>
\definecolor{ACATtan}{RGB}{201,159,109}
\definecolor{ACATdtan}{RGB}{179,72,18}
\definecolor{ACATlblue}{RGB}{24,146,200}
\definecolor{ACATdgreen}{RGB}{0,68,0}
\definecolor{ACATdbrown}{RGB}{65,14,23}
\definecolor{ACATbackground}{RGB}{255, 204, 102}

\setbeamerfont*{title page header}{size=\LARGE}
\setbeamercolor*{title page header}{fg=ACATdbrown}
\setbeamercolor*{author}{fg=ACATdbrown}
\setbeamercolor*{date}{fg=ACATdtan}
\setbeamercolor*{institute}{fg=ACATdgreen}

\setbeamercolor*{frametitle}{fg=ACATdbrown}
\setbeamercolor{block body}{bg=ACATbackground!10!white}
\setbeamercolor{block title}{bg=ACATbackground, fg=ACATdbrown}

\setbeamercolor{itemize item}{fg=ACATdgreen}
\setbeamertemplate{itemize item}[circle]

\setbeamercolor{section in head/foot}{fg=ACATlgreen}
\setbeamercolor{mini frame}{fg=ACATdtan}
\setbeamercolor{framefooter}{fg=ACATdtan}

\mode
<all>
